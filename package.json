{
  "name": "ssossotest",
  "version": "0.0.1",
  "description": "ssossotest by ssosso.table",
  "productName": "ssossotest",
  "author": "bear-frog",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ssossotest",
    "format": "prettier --write \\\"**/*.{js,ts,vue,scss,html,md,json}\\\" --ignore-path ssossotest/.gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "_comment": "cross-env 모듈을 사용하여 개발 모드 인식",
    "dev": "cross-env NODE_ENV=production quasar dev -m ssr",
    "build": "cross-env NODE_ENV=production quasar build -m ssr",
    "build:pwa": "cross-env NODE_ENV=production build -m pwa",
    "start": "cross-env NODE_ENV=production pm2 start dist/ssr/index.js --watch"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "axios": "^0.21.1",
    "concurrently": "^7.6.0",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "mysql2": "^3.1.2",
    "nodemon": "^2.0.20",
    "pm2": "^5.2.2",
    "quasar": "^2.6.0",
    "socket.io": "^4.6.1",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.0.0",
    "@types/express": "^4.17.17",
    "@types/node": "^12.20.55",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^2.8.3",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
